{
  "name": "@ladjs/assets",
  "description": "Assets for Lad",
  "version": "1.0.1",
  "author": "Nick Baugh <niftylettuce@gmail.com> (http://niftylettuce.com/)",
  "bugs": {
    "url": "https://github.com/ladjs/assets/issues",
    "email": "niftylettuce@gmail.com"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "contributors": [
    "Nick Baugh <niftylettuce@gmail.com> (http://niftylettuce.com/)"
  ],
  "dependencies": {
    "clipboard": "^2.0.6",
    "is-string-and-not-blank": "^0.0.2",
    "qs": "^6.9.4",
    "spinkit": "1.x",
    "superagent": "^5.3.1",
    "sweetalert2": "8.x",
    "url-parse": "^1.4.7"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@commitlint/cli": "^9.1.1",
    "@commitlint/config-conventional": "^9.1.1",
    "@ladjs/browserslist-config": "^0.0.1",
    "cross-env": "^7.0.2",
    "eslint": "^7.6.0",
    "eslint-config-xo-lass": "^1.0.3",
    "fixpack": "^3.0.6",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "remark-cli": "^8.0.1",
    "remark-preset-github": "^3.0.0",
    "xo": "^0.32.1"
  },
  "engines": {
    "node": ">=8.3"
  },
  "files": [
    "lib",
    "scss"
  ],
  "homepage": "https://github.com/ladjs/assets",
  "husky": {
    "hooks": {
      "pre-commit": "npm test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "keywords": [
    "4",
    "ajax",
    "assets",
    "auto",
    "beta",
    "bootstrap",
    "clipboard",
    "confirm",
    "custom",
    "file",
    "fix",
    "form",
    "frontend",
    "hash",
    "input",
    "jump",
    "lad",
    "lass",
    "on",
    "prompt",
    "return",
    "scroll"
  ],
  "license": "MIT",
  "lint-staged": {
    "linters": {
      "*.js": [
        "xo --fix",
        "git add"
      ],
      "*.md": [
        "remark . -qfo",
        "git add"
      ],
      "package.json": [
        "fixpack",
        "git add"
      ]
    }
  },
  "main": "lib/index.js",
  "prettier": {
    "singleQuote": true,
    "bracketSpacing": true,
    "trailingComma": "none"
  },
  "remarkConfig": {
    "plugins": [
      "preset-github"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ladjs/assets"
  },
  "scripts": {
    "build": "babel src --out-dir lib --source-maps",
    "lint": "xo && remark . -qfo",
    "test": "npm run lint && npm run build",
    "watch": "babel src --watch --out-dir lib --source-maps"
  },
  "xo": {
    "prettier": true,
    "space": true,
    "extends": [
      "xo-lass"
    ],
    "env": [
      "node",
      "browser",
      "jquery"
    ]
  }
}
