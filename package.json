{
  "name": "@ladjs/assets",
  "description": "Assets for Lad",
  "version": "0.0.3",
  "author": "Nick Baugh <niftylettuce@gmail.com> (http://niftylettuce.com/)",
  "bugs": {
    "url": "https://github.com/ladjs/assets/issues",
    "email": "niftylettuce@gmail.com"
  },
  "contributors": [
    "Nick Baugh <niftylettuce@gmail.com> (http://niftylettuce.com/)"
  ],
  "dependencies": {
    "clipboard": "^2.0.4",
    "frisbee": "^2.0.9",
    "lodash": "^4.17.11",
    "qs": "^6.7.0",
    "sweetalert2": "^8.9.0",
    "underscore.string": "^3.3.5"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-plugin-compat": "^3.1.1",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^2.2.0",
    "lint-staged": "^8.1.5",
    "prettier": "^1.17.0",
    "remark-cli": "^6.0.1",
    "remark-preset-github": "^0.0.13",
    "xo": "^0.24.0"
  },
  "engines": {
    "node": ">=8.3"
  },
  "homepage": "https://github.com/ladjs/assets",
  "keywords": [
    "lad",
    "lass",
    "assets",
    "frontend",
    "clipboard",
    "return",
    "hash",
    "fix",
    "facebook",
    "scroll",
    "on",
    "custom",
    "file",
    "input",
    "bootstrap",
    "beta",
    "4",
    "ajax",
    "form",
    "confirm",
    "prompt",
    "jump",
    "auto"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.{js,jsx,mjs,ts,tsx,css,less,scss,json,graphql}": [
      "prettier --write --single-quote --trailing-comma none",
      "git add"
    ],
    "*.md": [
      "remark . -qfo",
      "git add"
    ]
  },
  "main": "lib/index.js",
  "remarkConfig": {
    "plugins": [
      "preset-github"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ladjs/assets"
  },
  "xo": {
    "extends": "prettier",
    "plugins": [
      "prettier"
    ],
    "env": [
      "browser",
      "jquery"
    ],
    "parserOptions": {
      "sourceType": "script"
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "bracketSpacing": true,
          "trailingComma": "none"
        }
      ],
      "max-len": [
        "error",
        {
          "code": 80,
          "ignoreUrls": true
        }
      ],
      "capitalized-comments": "off",
      "camelcase": "off",
      "no-warning-comments": "off"
    },
    "space": true
  },
  "scripts": {
    "lint": "xo && eslint lib && remark . -qfo",
    "precommit": "lint-staged && npm test",
    "test": "npm run build && npm run lint",
    "build": "node_modules/.bin/babel src --out-dir lib",
    "watch": "node_modules/.bin/babel src --watch --out-dir lib"
  }
}
