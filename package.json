{
  "name": "@ladjs/assets",
  "description": "Assets for Lad",
  "version": "0.0.2",
  "author": "Nick Baugh <niftylettuce@gmail.com> (http://niftylettuce.com/)",
  "bugs": {
    "url": "https://github.com/ladjs/assets/issues",
    "email": "niftylettuce@gmail.com"
  },
  "contributors": [
    "Nick Baugh <niftylettuce@gmail.com> (http://niftylettuce.com/)"
  ],
  "dependencies": {
    "clipboard": "^1.7.1",
    "frisbee": "^1.6.4",
    "lodash": "^4.17.4",
    "qs": "^6.5.1",
    "sweetalert2": "^7.1.3",
    "underscore.string": "^3.3.4"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-lodash": "^3.3.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "cross-env": "^5.0.5",
    "eslint": "^4.13.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-compat": "^2.1.0",
    "eslint-plugin-prettier": "^2.4.0",
    "husky": "^0.14.3",
    "lint-staged": "^4.0.4",
    "prettier": "^1.6.1",
    "remark-cli": "^4.0.0",
    "remark-preset-github": "^0.0.6",
    "xo": "^0.18.2"
  },
  "engines": {
    "node": ">=8.3"
  },
  "homepage": "https://github.com/ladjs/assets",
  "keywords": [
    "lad",
    "lass",
    "assets",
    "frontend",
    "clipboard",
    "return",
    "hash",
    "fix",
    "facebook",
    "scroll",
    "on",
    "custom",
    "file",
    "input",
    "bootstrap",
    "beta",
    "4",
    "ajax",
    "form",
    "confirm",
    "prompt",
    "jump",
    "auto"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.{js,jsx,mjs,ts,tsx,css,less,scss,json,graphql}": [
      "prettier --write --single-quote --trailing-comma none",
      "git add"
    ],
    "*.md": ["remark . -qfo", "git add"]
  },
  "main": "lib/index.js",
  "remarkConfig": {
    "plugins": ["preset-github"]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ladjs/assets"
  },
  "xo": {
    "extends": "prettier",
    "plugins": ["prettier"],
    "env": ["browser", "jquery"],
    "parserOptions": {
      "sourceType": "script"
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "bracketSpacing": true,
          "trailingComma": "none"
        }
      ],
      "max-len": [
        "error",
        {
          "code": 80,
          "ignoreUrls": true
        }
      ],
      "capitalized-comments": "off",
      "camelcase": "off",
      "no-warning-comments": "off"
    },
    "space": true
  },
  "scripts": {
    "lint": "xo && eslint lib && remark . -qfo",
    "precommit": "lint-staged && npm test",
    "test": "npm run build && npm run lint",
    "build": "node_modules/.bin/babel src --out-dir lib",
    "watch": "node_modules/.bin/babel src --watch --out-dir lib"
  }
}
